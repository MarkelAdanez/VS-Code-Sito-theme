		"tab.lastPinnedBorder": "#151524",
		"sideBar.background": "#20203d",
		"statusBar.background": "#111122",
		"statusBar.noFolderBackground": "#182038",
		"statusBarItem.remoteBackground": "#20203d",
		"ports.iconRunningProcessForeground": "#6e68c4",
		"editorGroupHeader.tabsBackground": "#20203d",
		"titleBar.activeBackground": "#060b16",
		"titleBar.inactiveBackground": "#15181d",
		"selection.background": "#a0afcca1",
		// editor
		"editor.background": "#2E4053",
		"editorGroup.border": "#29274444",
		"editorCursor.foreground": "#68b3df", //970000
		"editor.foreground": "#b3bed8",
